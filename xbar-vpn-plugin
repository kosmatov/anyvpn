#!/bin/bash

# Connect/disconnect OpenVPN

# <xbar.title>OpenVPN</xbar.title>
# <xbar.version>v1.0</xbar.version>
# <xbar.author>Konstantin Kosmatov</xbar.author>
# <xbar.author.github>kosmatov</xbar.author.github>
# <xbar.desc>Displays status of an OpenVPN connection with option to connect/disconnect.</xbar.desc>
# <xbar.image>https://github.com/kosmatov/blob/master/xbar-plugin.png</xbar.image>

IMAGE_DISCONNECTED=""
IMAGE_CONNECTING=""
IMAGE_CONNECTED=""

case "$1" in
  connect)
    make -C __workdir__ connect
    ;;
  disconnect)
    make -C __workdir__ disconnect
    ;;
esac

state=$(make -C __workdir__ state)
case "$state" in
  CONNECTED)
    echo " | templateImage=$IMAGE_CONNECTED"
    echo '---'
    echo "Disconnect VPN | bash='$0' param1=disconnect terminal=false refresh=true"
    ;;
  AUTH)
    make -C __workdir__ check-password-need > /dev/null 2>&1 && make -C __workdir__ connect
    ;;
  "")
    echo " | templateImage=$IMAGE_DISCONNECTED"
    echo '---'
    echo "Connect VPN | bash='$0' param1=connect terminal=false refresh=true"
    ;;
  *)
    echo " | templateImage=$IMAGE_CONNECTING"
    echo '---'
    echo "$state..."
    echo "Reconnect VPN | bash='$0' param1=connect terminal=false refresh=true"
    ;;
esac
